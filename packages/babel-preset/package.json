{
  "name": "@geospatial/development-babel-preset",
  "types": "src/index.ts",
  "publishConfig": {
    "types": "lib/index.d.ts"
  },
  "version": "19.10.0",
  "private": true,
  "description": "Sterblue standard babel preset",
  "license": "UNLICENSED",
  "author": "Vincent Lecrubier <vincent.lecrubier@sterblue.com>",
  "main": "lib/index.cjs.js",
  "scripts": {
    "build:deps": "ultra -r --build --filter + build:lib",
    "build": "yarn build:rollup && yarn build:types",
    "build:babel": "babel src --out-dir lib --config-file ./.babelrc.js  --extensions \".ts,.tsx\" --source-maps inline",
    "build:lib": "rollup -c",
    "build:rollup": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly ",
    "clean": "rm -rf lib"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-export-default-from": "7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "7.12.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@emotion/babel-plugin": "11.0.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-imports": "2.0.0",
    "builtin-modules": "3.1.0",
    "lodash": "4.17.20"
  },
  "devDependencies": {
    "@babel/cli": "7.6.4",
    "@babel/core": "7.11.5",
    "@babel/polyfill": "7.6.0",
    "@babel/runtime": "7.6.3",
    "@rollup/plugin-babel": "5.2.2",
    "@rollup/plugin-commonjs": "17.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "11.0.1",
    "@types/jest": "24.0.13",
    "@types/react": "17.0.0",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-preset-react-app": "10.0.0",
    "core-js": "3.6.5",
    "rollup": "2.35.1",
    "rollup-plugin-pnp-resolve": "2.0.0",
    "rollup-plugin-postcss": "4.0.0",
    "ts-pnp": "1.2.0",
    "typescript": "4.1.3",
    "webpack": "4.41.6"
  }
}
